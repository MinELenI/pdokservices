      <Keyword>Bos</Keyword>
      <Keyword>Grondverwerving/grondzaken</Keyword>
      <Keyword>Landbouw</Keyword>
      <Keyword>Landschap</Keyword>
      <Keyword>Natuur</Keyword>
      <Keyword>Recreatie</Keyword>
      <Keyword>Wettelijke begrenzing (beleid)</Keyword>
      <Keyword>Kadastrale grenzen</Keyword>
      <Keyword vocabulary="ISO">infoMapAccessService</Keyword>
